<%= form_for @user do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </p>
    <p>
        <%= f.label :phone_number %>
        <%= f.text_field :phone_number %>
    </p>
    <p>
        <%= f.label :address %>
        <%= f.text_field :address %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.text_field :password_digest %>
    </p>
        <%= f.submit %>
<% end %> 