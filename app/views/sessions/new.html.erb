<%= form_for :session, url: sessions_path do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.text_field :password %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>